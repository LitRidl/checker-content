<?xml version="1.0" encoding="utf-8" ?>
<problem package="ru.ejudge.sample_contest" id="{{id}}" type="standard">
  <statement language="ru_RU">
    <title>Вариант &numero;{{id}}</title>

    <description><p>{{task}}</p><br/>
    </description>

    <input_format><p>Во входной строке {{input_type}}</p><br/>
    </input_format>

    <output_format><p>Ответ ({{answer_type}}) должен выводиться через пробел после входных данных, причём модификация исходных данных запрещена.</p><br/>
    </output_format>
  </statement>

  <examples>
    <example>
      <input><pre>{{test_in}}</pre></input>
      <output><pre>{{test_out}}</pre>
      </output>
    </example>
  </examples>
</problem>
